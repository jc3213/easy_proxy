{
    "name": "Easy Proxy",
    "description": "An easy proxy server switcher",
    "version": "0.1.0",
    "manifest_version": 3,
    "permissions": [
        "proxy",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "options_ui": {
        "page": "pages/options/options.html",
        "open_in_tab": true
    }
}
